<div class="container text-center">
   <div class="card-group">
      <div class="row">
         {{#each suplier}}
         <div class="card col-md-3 m-3">
            <div class="card-body">
               <a href="/suplier/detail/{{_id}}"><h4 class="card-title text-success">{{Name}}</h4></a>
               <p class="card-text text-danger">{{Address}}</p>
            </div>
         </div>
         {{/each}}
      </div>
   </div>
</div>